<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#dadada"
    android:orientation="vertical"
    android:id="@+id/hellohi_hometutor"
    tools:context=".Attendance_record_hometutor">

    <!--  <include
             layout="@layout/toolbar"
             android:id="@+id/classes_toolbar"
             />
       -->
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_r_ht"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:elevation="5dp"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"

        />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/attendance_record_recyclerview_ht"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/toolbar_r_ht"/>



</RelativeLayout>
